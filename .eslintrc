{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "plugins": ["prettier" ,"react"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0
  }
}